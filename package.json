  "scripts": {
    "test": "npm run test:server",
    "test:watch": "npm run test:server:watch",
    "test:server": "mocha-webpack --webpack-config server/webpack.config.js \"server/**/test/*.test.js\"",
    "test:server:watch": "mocha-webpack --webpack-config server/webpack.config.js --watch --growl \"server/**/test/*.test.js\"",
