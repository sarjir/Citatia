    "static": "./static.sh",
    "build": "npm run static && npm run build:client && npm run build:server",
    "build:client": "webpack --progress --colors",
    "build:server": "webpack --progress --colors --config server/webpack.config.js",
    "dev": "npm run static; webpack --watch --progress --colors",
    "start": "node build/js/server.js",
    "start:dev": "nodemon build/js/server.js"
  },
